{
    "openapi": "3.0.0",
    "info": {
        "title": "User endpoints",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "localhost:8000"
        }
    ],
    "paths": {
        "/api/login": {
            "post": {
                "summary": "Inicia Sesion",
                "operationId": "App\\Http\\Controllers\\UserController::login",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "description": "Email",
                        "required": true
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "Contrase√±a de minimo 6 caracteres",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Genera Token"
                    },
                    "401": {
                        "description": "Usuario no autorizado"
                    },
                    "default": {
                        "description": "Server error"
                    }
                }
            }
        }
    },
    "security": [
        []
    ]
}